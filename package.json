{
  "adonisjs": {
    "providers": [
      "adonis-grpc-consumer"
    ],
    "templates": {
      "config": [
        "grpc-consumer.txt"
      ]
    },
    "types": "adonis-grpc-consumer"
  },
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "bin": {
    "build-proto": "./node_modules/@grpc/proto-loader/build/bin/proto-loader-gen-types.js"
  },
  "bugs": {
    "url": "https://github.com/Julien-R44/adonis-grpc-consumer/issues"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.9"
  },
  "description": "Adonis gRPC client provider for easily communicate with gRPC services",
  "devDependencies": {
    "@adonisjs/core": "^5.7.5",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.11",
    "@types/node": "^17.0.30",
    "copyfiles": "^2.4.1",
    "del-cli": "^4.0.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "japa": "^4.0.0",
    "mrm": "^3.0.10",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src",
    "build/templates"
  ],
  "homepage": "https://github.com/Julien-R44/adonis-grpc-consumer#readme",
  "keywords": [
    "adonis5",
    "adonisjs",
    "grpc"
  ],
  "license": "MIT",
  "main": "build/providers/GrpcConsumerProvider.js",
  "name": "adonis-grpc-consumer",
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 100,
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "useTabs": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-R44/adonis-grpc-consumer.git"
  },
  "scripts": {
    "build": "npm run compile && npm run copyfiles",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "copyfiles": "copyfiles \"templates/**/*.txt\" build",
    "format": "prettier --write .",
    "lint": "eslint . --ext=.ts",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "prepublishOnly": "npm run build",
    "pretest": "npm run lint",
    "test": "node .bin/test.js"
  },
  "typings": "./build/adonis-typings/index.d.ts",
  "version": "0.1.1"
}
